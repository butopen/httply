{
  "useTabs": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "printWidth": 100,
  "plugins": [
    "./node_modules/prettier-plugin-tailwindcss"
  ],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte",
        "svelteSortOrder": "options-styles-scripts-markup",
        "svelteBracketNewLine": true
      }
    }
  ]
}
